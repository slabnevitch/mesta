extends _misc/main

block title
	| Страница выдачи

block content
	.page-search-resuls
		section.place
			.container
				.crumbs
					a.crumbs_back(href='#')
						span.icon-back
				
				.title-with-dd
					span Куда поехать из
					a(href="#") Нижний Новгород

				include _blocks/_filter-tags

				.results-filter
					.results-filter__items
						.results-filter__item.item-results-filter.results-filter__item--services
							.item-results-filter__name Услуги
							.icon-faq_arrow
							.item-results-filter__dd
								-var services = [{name: 'Кафе на территории', icon: 'place_service1'}, {name: 'Проживание', icon: 'residence'}, {name: 'Беседки / Мангальная зона ', icon: 'place_service2'}, {name: 'Дневное посещение', icon: 'daylight'}, {name: 'Баня', icon: 'bath'}, {name: 'Spa программы', icon: 'spa'}, {name: 'Бассейн', icon: 'spa'}, {name: 'Пляж', icon: 'beach'}, {name: 'Зоодвор', icon: 'zoo'}, {name: 'Рыбалка', icon: 'fishing'}, {name: 'Развлечения для детей', icon: 'carousel'}, {name: 'Конные прогулки', icon: 'horse'}, {name: 'Можно с питомцами', icon: 'place_service3'}];
								.place_service-items
									each item, ind in services
										div(class=ind === 9 ? "place_service-item disabled" : "place_service-item")
											span(class="place_service-item-icon icon-" + item.icon)
											= item.name

						.results-filter__item.item-results-filter.results-filter__item--dates
							.item-results-filter__name Свободные даты
							.icon-faq_arrow
							.item-results-filter__dd
								input#datepicker-static.form_inp(type='text' name='' readonly='')
								input#datepicker-static2.form_inp(type='text' name='' readonly='')
								
								.item-results-filter__buttons
									a(href="#").btn2.btn-sm Очистить
									a(href="#").btn.btn-sm Выбрать

						.results-filter__item.item-results-filter
							.item-results-filter__name Расстояние от города
							.icon-faq_arrow
							.item-results-filter__dd
								.item-results-filter__caption Расстояние от города
								.range
									.range-top
										input(autocomplete="off" type="text" name="" value="" class="range__input" id="minval")
										input(autocomplete="off" type="text" name="" value="" class="range__input" id="maxval")
									.range-slider

								a(href="#").btn.btn-sm Применить

						.results-filter__item.item-results-filter
							.item-results-filter__name Сортировать по:
								span.item-results-filter__sort-key возрастанию цены
							.icon-faq_arrow
							.item-results-filter__dd
								.item-results-filter__caption Сортировать по:
								form.item-results-filter__form
									-var sorts = ['Рекомендуемые', 'По возрастанию цены', 'По убыванию цены', 'По расстоянию'];
									each item, ind in sorts
										if(ind === 1)
											label.item-results-filter__label
												input(type="radio" name="sort-value" class="item-results-filter__input" value=item checked)
												span.item-results-filter__radio
												span.item-results-filter__radio-value= item
										else
											label.item-results-filter__label
												input(type="radio" name="sort-value" class="item-results-filter__input" value=item)
												span.item-results-filter__radio
												span.item-results-filter__radio-value= item
								
								a(href="#").btn.btn-sm Применить

					.results-filter__map-open.results-filter__item 
						span.icon-place_contacts-item2
						| Показать на карте

		.resuls-map
			.resuls-map__close
				span.icon-delete
			#map.resuls-map__map
			.resuls-map__bottom
				include _blocks/_filter-tags

